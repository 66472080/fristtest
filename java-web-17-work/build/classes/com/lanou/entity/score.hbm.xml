<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC     
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"    
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.lanou.entity.score.hbm.xml" table="score">
		<id name="id" type="int" column="id">
			<generator class="identity"></generator>
		</id>
		<property name="sid" type="int" column="sid"></property>
		<property name="score" type="int" column="score"></property>
		<property name="time" type="string" column="time"></property>
	</class>
</hibernate-mapping>